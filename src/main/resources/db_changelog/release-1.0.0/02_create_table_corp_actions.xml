<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">

<changeSet id="create_table_corp_actions" author="borisov-tyu">
    <preConditions onFail="MARK_RAN">
        <not>
            <tableExists tableName="corp_actions"/>
        </not>
    </preConditions>
    <createTable tableName="corp_actions">
        <column name="caid" type="BIGINT">
            <constraints nullable="false" primaryKey="true" primaryKeyName="corp_actions_pkey"/>
        </column>
        <column name="catype" type="VARCHAR(128)">
            <constraints nullable="false"/>
        </column>
        <column name="reference" type="VARCHAR(20)">
            <constraints nullable="false"/>
        </column>
        <column name="swift_type" type="VARCHAR(8)">
            <constraints nullable="false"/>
        </column>
        <column name="ma_vo_code" type="VARCHAR(20)">
            <constraints nullable="false"/>
        </column>
        <column name="date_reg_owners" type="DATE">
            <constraints nullable="true"/>
        </column>
        <column name="isin" type="VARCHAR(25)">
            <constraints nullable="false"/>
        </column>
        <column name="date_start" type="DATE">
            <constraints nullable="true"/>
        </column>
        <column name="date_end" type="DATE">
            <constraints nullable="true"/>
        </column>
        <column name="sfkpg_acct" type="varchar(25)" >
            <constraints nullable="false"/>
        </column>
        <column name="org_nm" type="varchar(40)" >
            <constraints nullable="false"/>
        </column>
    </createTable>
</changeSet>
</databaseChangeLog>