<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">

    <changeSet id="create_table_client_acc" author="borisov-tyu">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="client_acc"/>
            </not>
        </preConditions>
        <createTable tableName="client_acc">
            <column name="cftid" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="acc_depo" type="VARCHAR(20)">
                <constraints nullable="false"/>
            </column>
            <column name="sub_acc_depo" type="VARCHAR(40)">
                <constraints nullable="true"/>
            </column>
        </createTable>
        <addPrimaryKey
                tableName="client_acc"
                columnNames="acc_depo"
                constraintName="client_acc_pkey"/>
    </changeSet>
</databaseChangeLog>